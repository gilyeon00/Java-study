package day01;

public class OpTest {

	public static void main(String[] args) {
		int a = 100;
		int b = 3;
		
		System.out.println("a+b: " + a+b);
//		System.out.println("a-b:" + a-b);
		System.out.println("a*b: " + a*b);
		System.out.println("a/b: " + a/b);
		System.out.println("a%b: " + a%b);
		System.out.println("a++: " + a++);
		System.out.println("a : " + a);
		System.out.println("b++: " + b++);
		System.out.println("b : " + b);
		
//		System.out.println("a>b: " + a>b);
		System.out.println(a>b && --a>1000);

	}

}
